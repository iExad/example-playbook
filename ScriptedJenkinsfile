node("ansible_docker"){
    stage("Git checkout"){
        git credentialsId: 'e03cf29a-af20-43b1-a1d8-181b28c58a78', url: 'git@github.com:iExad/example-playbook.git'
    }
    stage("Check ssh key"){
        secret_check=true
    }
    stage("Run playbook"){
        if (secret_check){
            sh 'ansible-playbook site.yml -i inventory/prod.yml'
        }
        else{
            echo 'no more keys'
        }
        
    }
}
