node("ansible_docker"){
    stage("Git checkout"){
        git branch: 'main', credentialsId: 'c9d64cba-9b33-42a8-ab81-982468ab660b', url: 'git@github.com:iExad/example-playbook.git'
    }
    stage("Check ssh key"){
        secret_check=true
    }
    stage("Run playbook"){
        if (secret_check){
            sh 'ansible-playbook site.yml -i inventory/prod.yml'
        }
        else{
            echo 'no more keys'
        }
        
    }
}
